        </Feature>

  </Feature>
</Feature>
    <UI>
      <ui:WixUI Id="WixUI_FeatureTree" />
      <Property Id="WIXUI_INSTALLDIR" Value="APPLICATIONROOTFOLDER" />
      <UIRef Id="WixUI_ErrorProgressText" />
